@{
    Layout = null;
}
@using Medicio.DTOs.Account
@model RegisterDto

<head>
    <link href="~/manage/css/sb-admin-2.min.css" rel="stylesheet">
</head>
<div class="container justify-content-center mt-5">
    <form method="post" >
        <div class="mb-3">
            <label asp-for="FullName" class="form-label">FullName</label>
            <input asp-for="FullName" class="form-control">
            <span asp-validation-for="FullName" class="text-danger"></span>
        </div>

        <div class="mb-3">
            <label asp-for="Email" class="form-label">Email</label>
            <input asp-for="Email" class="form-control">
            <span asp-validation-for="Email" class="text-danger"></span>
        </div>

        <div class="mb-3">
            <label asp-for="UserName" class="form-label">UserName</label>
            <input asp-for="UserName" class="form-control">
            <span asp-validation-for="UserName" class="text-danger"></span>
        </div>

        <div class="mb-3">
            <label asp-for="Password" class="form-label">Passwrod</label>
            <input asp-for="Password" class="form-control">
            <span asp-validation-for="Password" class="text-danger"></span>
        </div>
        <div class="mb-3">
            <label asp-for="ConPassword" class="form-label">ConPasswrod</label>
            <input asp-for="ConPassword" class="form-control">
            <span asp-validation-for="ConPassword" class="text-danger"></span>
        </div>
        <div asp-validation-summary=All>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>